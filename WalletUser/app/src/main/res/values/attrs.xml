<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="WheelProgress">

        <attr name="itemColor" format="color"></attr>
        <attr name="itemWidth" format="integer"></attr>
        <attr name="itemCornersRadius" format="float"></attr>
        <attr name="oneSeftRotate" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="RangeSeekBar">
        <attr name="max" format="float"/>
        <attr name="min" format="float"/>
        <attr name="reserve" format="float"/>
        <attr name="cells" format="integer"/>
        <attr name="lineColorSelected" format="color"/>
        <attr name="lineColorEdge" format="color"/>
        <attr name="seekBarResId" format="reference"/>
    </declare-styleable>

</resources>